{
  "platforms": ["android", "ios"],
  "android": {
    "modules": ["expo.modules.androidauto.AndroidAutoModule"]
  },
  "ios": {
    "modules": ["expo.modules.carplay.CarPlayModule"]
  },
  "dependencyOnlyInjection": {
    "supported": true,
    "android": {
      "manifestQueries": [
        {
          "intent": {
            "action": "androidx.car.app.CarAppService"
          }
        }
      ],
      "permissions": [
        "com.google.android.gms.permission.CAR_APPLICATION"
      ]
    }
  }
}
